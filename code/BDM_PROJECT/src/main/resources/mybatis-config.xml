<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- mybatis 설정 -->  
<configuration>
    <!-- mapUnderscoreToCamelCase 맵핑 -->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="logImpl" value="LOG4J2"/>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
    <typeAliases>
        <typeAlias type="com.test.bdm.user.domain.UserVO"			alias="UserVO"/>
        <typeAlias type="com.test.bdm.nutrient.domain.NutrientVO"	alias="NutrientVO"/>
        <typeAlias type="com.test.bdm.bulletin.domain.BulletinVO"	alias="BulletinVO"/>
        <typeAlias type="com.test.bdm.notice.domain.NoticeVO"		alias="NoticeVO"/>
        <typeAlias type="com.test.bdm.news.domain.NewsVO"			alias="NewsVO"/>
        <typeAlias type="com.test.bdm.code.domain.CodeVO"			alias="CodeVO"/>
        <typeAlias type="com.test.bdm.file.domain.FileVO"			alias="FileVO"/>
        <typeAlias type="com.test.bdm.qa.domain.QaVO"				alias="QaVO"/>
        <typeAlias type="com.test.bdm.comments.domain.CommentsVO"	alias="CommentsVO"/>
        <typeAlias type="com.test.bdm.comments.domain.QaCommentsVO"	alias="QaCommentsVO"/>
        <typeAlias type="com.test.bdm.nutrient.domain.EatVO"        alias="EatVO"/>
        <typeAlias type="com.test.bdm.heart.domain.HeartVO"         alias="HeartVO"/>
        <typeAlias type="com.test.bdm.cmn.DTO"                      alias="DTO"/>
        
    </typeAliases>
</configuration>